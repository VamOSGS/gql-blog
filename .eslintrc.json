{
  "parser": "babel-eslint",
  "extends": ["plugin:flowtype/recommended", "airbnb-base"],
  "plugins": ["flowtype"],
  "rules": {
    "require-jsdoc": 0,
    "no-unused-vars": "warn",
    "flowtype/boolean-style": 0,
    "no-console": 0,
    "global-require": 0,
    "object-curly-spacing": 0,
    "eqeqeq": 0
  }
}
